#!/bin/bash -e
. /etc/advligorts/env
sys="$1"
if [ -z "$sys" ] ; then
    echo "usage: $(basename $0) <sys>"
    exit 1
fi
export EPICS_DB_INCLUDE_PATH=/usr/lib/epics/dbd
TARGET=/opt/rtcds/${SITE,,*}/${IFO,,*}
ETARGET=${TARGET}/target/${sys}/${sys}epics
exec ${ETARGET}/${sys}epics ${sys}epics${IFO}.cmd
