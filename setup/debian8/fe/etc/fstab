# /etc/fstab: static file system information.
#
#   - for diskless front-end
# NFS mounts
x2boot0:/diskless/root           /        nfs    sync,hard,intr,rw,nolock   0 0
#/dev/nfs	                 /         nfs     sync,hard,intr,rw,nolock    0 0
x2boot0:/opt                     /opt     nfs     async,hard,intr,rw,nolock   0 0
x2boot0:/home                    /home     nfs     async,hard,intr,rw,nolock   0 0
x2fs:/opt/cdscfg              /opt/cdscfg      nfs     async,hard,intr,rw,nolock   0 0
x2fs:/opt/rtapps              /opt/rtapps      nfs     async,hard,intr,rw,nolock   0 0
x2fs:/opt/rtcds               /opt/rtcds      nfs     async,hard,intr,rw,nolock   0 0
# -- other mounts --
proc                            /proc		proc    defaults          0 0
tmpfs				/run		tmpfs	rw    0 0
tmpfs				/var		tmpfs	rw    0 0
tmpfs                           /tmp            tmpfs   rw    0 0
